{
  "scripts": {
    "clean": "rimraf docs",
    "copy:CNAME": "cpx src/CNAME docs",
    "copy": "npm-run-all -p copy:*",
    "watch:html": "cpx -w src/index.html docs",
    "watch:assets": "cpx -w \"src/img/**/*\" docs/img",
    "watch:js": "cpx -w src/main.js docs",
    "watch:elm": "elm-live src/Main.elm --open --start-page=index.html --dir=docs --pushstate -- --output=docs/elm.js",
    "watch:sass": "sass --watch src:docs",
    "watch": "npm-run-all -p watch:*",
    "prewatch": "cpx src/index.html docs && sass src:docs",
    "compile:html": "cpx src/index.html docs",
    "compile:assets": "cpx \"src/img/**/*\" docs/img",
    "compile:js": "cpx src/main.js docs",
    "compile:elm": "elm make src/Main.elm --optimize --output=docs/elm.optimized.js",
    "compile:sass": "sass --style=compressed --no-source-map src:docs",
    "compile": "npm-run-all -p compile:*",
    "minify:elm": "google-closure-compiler --js=docs/elm.optimized.js --js_output_file=docs/elm.js",
    "postminify:elm": "rimraf docs/elm.optimized.js",
    "build": "npm-run-all -s clean copy compile minify:elm",
    "start": "npm-run-all -s clean copy watch"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.2",
    "elm-live": "3.4.0",
    "google-closure-compiler": "^20190929.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "sass": "^1.22.10"
  },
  "dependencies": {}
}
