{
  "scripts": {
    "clean": "rimraf dist/elm.js",
    "watch": "elm-live src/elm/Main.elm --open -- --debug --output=dist/elm.js",
    "compile": "elm make src/elm/Main.elm --optimize --output=dist/elm.optimized.js",
    "minify": "google-closure-compiler --js=dist/elm.optimized.js --js_output_file=dist/elm.js",
    "postminify": "rimraf dist/elm.optimized.js",
    "build": "npm-run-all -s clean compile minify",
    "start": "npm-run-all -s clean watch"
  },
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.4",
    "elm-live": "^4.0.2",
    "google-closure-compiler": "^20201102.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {}
}
